{
  "compilerOptions": {
    "sourceMap": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "lib": ["es5", "es6", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "declaration": true,
    "target": "es5",
    "declarationMap": true,
    "preserveSymlinks": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "typeRoots": ["node_modules/@adsktypes", "node_modules/@types"]
  },
  "exclude": [
    "node_modules",
    "node_modules_prodonly",
    "coverage",
    ".out",
    "build",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest",
    "src/setupTests.ts"
  ]
}
